

<div class="container">

	<div class="row">
	    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
			<%= form_for(:user ,:url=>{:action =>'create'},:role=>"form") do |f| %>
				<h1>Git URL  </h1>
				<hr >
				
				<div class="form-group">			
					<%= f.text_field(:myurl,{ :class=>"form-control input-lg",:type=>"url", :tabindex=>"0" ,:placeholder=>"Enter any Git Url...."})%>
				</div>

				<hr >
				
					<div class="form-group"><%= submit_tag("Add Url",{:class=>"btn btn-success btn-block btn-lg"})%>
					</div>
				
			<% end %>
		</div>
	</div>
	
</div>

